<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  </head>
  <body>
    <div>
      <!-- Navbar static page -->
      <nav class="navbar navbar-default bg-orange hidden-xs" id="navbar-sitelinks" style="margin-bottom:0;border-bottom:solid 1px #DDD">
        <div class="container">
          <div class="navbar-header">
            <a href="/"><img src="images/logo/logo.png" class="logo" alt="Logo Mulmut"></a>
          </div>
          <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="fs-10 ma-0"><a href="/">Manage Products</a></li>
            </ul>
          </div>
        </div>
      </nav>
    </div>

    <!-- Banner -->
    <section id="banner">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            <div class="inner">
              <h1>Jangan Lari Dari Kenyataan</h1>
              <button type="button" class="btn btn-primary btn-lg">BELI SEPATU MAHAL DULU!</button>
              <a href="#join" class="btn btn-lg"></a>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Products -->
    <section id="products">
      <div class="container">
        <div class="row">
          <h1>Pilihan Sepatu Mahal Dan Bocengli</h1>
          <div class="col-xs-6 col-sm-4 col-md-3">
            <div class="image">
              <img src="images/products/adidas-superstar.jpg">
            </div>
            <div class="description">
              <h3>Adidas Superstar</h3>
              <h4>Rp 2.000.000</h4>
            </div>
            <button type="button" class="btn btn-danger">Lihat Produk</button>
          </div>
        </div>
      </div>
    </section>

    <!-- Footer -->
    <div class="footer">
      <div class="container">
        <p>Copyright &copy; 2016 Tokotap. All rights reserved.</p>
      </div>
    </div>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
  </body>
</html>
